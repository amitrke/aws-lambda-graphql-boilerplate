{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "dom", "es2016", "esnext.asynciterable"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "sourceMap": false,
    "outDir": "lib"
  },
  "exclude": [
    "node_modules"
  ]
}
